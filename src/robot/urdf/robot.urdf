<?xml version="1.0"?>
<robot name="mobile_manipulator">

  <!-- ============================================ -->
  <!-- BASE LINK (Mobile Platform Body) -->
  <!-- ============================================ -->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.4 0.3 0.1"/>
      </geometry>
      <material name="blue">
        <color rgba="0.2 0.4 0.8 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.4 0.3 0.1"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
  </link>

  <!-- ============================================ -->
  <!-- WHEELS (4-wheel mobile base) -->
  <!-- ============================================ -->
  
  <!-- Front Left Wheel -->
  <joint name="front_left_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="front_left_wheel_link"/>
    <origin xyz="0.15 0.2 0" rpy="1.5708 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>
  
  <link name="front_left_wheel_link">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Front Right Wheel -->
  <joint name="front_right_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="front_right_wheel_link"/>
    <origin xyz="0.15 -0.2 0" rpy="1.5708 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>
  
  <link name="front_right_wheel_link">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Rear Left Wheel -->
  <joint name="rear_left_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="rear_left_wheel_link"/>
    <origin xyz="-0.15 0.2 0" rpy="1.5708 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>
  
  <link name="rear_left_wheel_link">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Rear Right Wheel -->
  <joint name="rear_right_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="rear_right_wheel_link"/>
    <origin xyz="-0.15 -0.2 0" rpy="1.5708 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>
  
  <link name="rear_right_wheel_link">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- ============================================ -->
  <!-- 6-AXIS ROBOTIC ARM -->
  <!-- ============================================ -->

  <!-- Joint 1: Base Rotation -->
  <joint name="arm_joint_1" type="revolute">
    <parent link="base_link"/>
    <child link="arm_base_link"/>
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-6.28" upper="6.28" effort="100" velocity="3.15"/>
    <!-- Webots-specific sensor tag -->
    <sensor name="arm_sensor_1" type="rotary">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent joint="arm_joint_1"/>
      <dynamics damping="1.0"/>
      <calibration rising="0.0"/>
      <resolution value="0.01"/>
    </sensor>
  </joint>

  <link name="arm_base_link">
    <visual>
      <origin xyz="0 0 0.06" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.12" radius="0.075"/>
      </geometry>
      <material name="dark_gray">
        <color rgba="0.15 0.15 0.2 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.06" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.12" radius="0.075"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.06" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Joint 2: Shoulder -->
  <joint name="arm_joint_2" type="revolute">
    <parent link="arm_base_link"/>
    <child link="shoulder_link"/>
    <origin xyz="0 0 0.06" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="100" velocity="3.15"/>
    <!-- Webots-specific sensor tag -->
    <sensor name="arm_sensor_2" type="rotary">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent joint="arm_joint_2"/>
      <dynamics damping="1.0"/>
      <calibration rising="0.0"/>
      <resolution value="0.01"/>
    </sensor>
  </joint>

  <link name="shoulder_link">
    <visual>
      <origin xyz="0 0 0.2" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.08 0.35"/>
      </geometry>
      <material name="arm_blue">
        <color rgba="0.2 0.35 0.55 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.2" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.08 0.35"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.2" rpy="0 0 0"/>
      <mass value="1.8"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Joint 3: Elbow -->
  <joint name="arm_joint_3" type="revolute">
    <parent link="shoulder_link"/>
    <child link="elbow_link"/>
    <origin xyz="0 0 0.375" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="100" velocity="3.15"/>
    <!-- Webots-specific sensor tag -->
    <sensor name="arm_sensor_3" type="rotary">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent joint="arm_joint_3"/>
      <dynamics damping="1.0"/>
      <calibration rising="0.0"/>
      <resolution value="0.01"/>
    </sensor>
  </joint>

  <link name="elbow_link">
    <visual>
      <origin xyz="0 0 0.15" rpy="0 0 0"/>
      <geometry>
        <box size="0.07 0.07 0.27"/>
      </geometry>
      <material name="arm_blue">
        <color rgba="0.2 0.35 0.55 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.15" rpy="0 0 0"/>
      <geometry>
        <box size="0.07 0.07 0.27"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.15" rpy="0 0 0"/>
      <mass value="1.2"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.005"/>
    </inertial>
  </link>

  <!-- Joint 4: Wrist Roll -->
  <joint name="arm_joint_4" type="revolute">
    <parent link="elbow_link"/>
    <child link="wrist_1_link"/>
    <origin xyz="0 0 0.285" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-6.28" upper="6.28" effort="50" velocity="3.2"/>
    <!-- Webots-specific sensor tag -->
    <sensor name="arm_sensor_4" type="rotary">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent joint="arm_joint_4"/>
      <dynamics damping="1.0"/>
      <calibration rising="0.0"/>
      <resolution value="0.01"/>
    </sensor>
  </joint>

  <link name="wrist_1_link">
    <visual>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.1" radius="0.03"/>
      </geometry>
      <material name="wrist_gray">
        <color rgba="0.18 0.18 0.22 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.1" radius="0.03"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
      <mass value="0.3"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <!-- Joint 5: Wrist Pitch -->
  <joint name="arm_joint_5" type="revolute">
    <parent link="wrist_1_link"/>
    <child link="wrist_2_link"/>
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-6.28" upper="6.28" effort="50" velocity="3.2"/>
    <!-- Webots-specific sensor tag -->
    <sensor name="arm_sensor_5" type="rotary">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent joint="arm_joint_5"/>
      <dynamics damping="1.0"/>
      <calibration rising="0.0"/>
      <resolution value="0.01"/>
    </sensor>
  </joint>

  <link name="wrist_2_link">
    <visual>
      <origin xyz="0 0 0.02" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.04"/>
      </geometry>
      <material name="wrist_light_gray">
        <color rgba="0.25 0.25 0.3 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.02" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.04"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.02" rpy="0 0 0"/>
      <mass value="0.2"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <!-- Joint 6: Wrist Yaw -->
  <joint name="arm_joint_6" type="revolute">
    <parent link="wrist_2_link"/>
    <child link="wrist_3_link"/>
    <origin xyz="0 0 0.02" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-6.28" upper="6.28" effort="50" velocity="3.2"/>
    <!-- Webots-specific sensor tag -->
    <sensor name="arm_sensor_6" type="rotary">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent joint="arm_joint_6"/>
      <dynamics damping="1.0"/>
      <calibration rising="0.0"/>
      <resolution value="0.01"/>
    </sensor>
  </joint>

  <link name="wrist_3_link">
    <visual>
      <origin xyz="0 0 0.0125" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.025" radius="0.035"/>
      </geometry>
      <material name="tool_flange">
        <color rgba="0.15 0.15 0.18 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.0125" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.025" radius="0.035"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.0125" rpy="0 0 0"/>
      <mass value="0.15"/>
      <inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005"/>
    </inertial>
  </link>

  <!-- Tool/Gripper Link (End Effector) -->
  <joint name="tool_joint" type="fixed">
    <parent link="wrist_3_link"/>
    <child link="tool0"/>
    <origin xyz="0 0 0.025" rpy="0 0 0"/>
  </joint>

  <link name="tool0">
    <visual>
      <origin xyz="0 0 0.03" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.04 0.06"/>
      </geometry>
      <material name="gripper">
        <color rgba="0.1 0.1 0.12 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.03" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.04 0.06"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.03" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

</robot>

